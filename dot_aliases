function dis(){ sudo "$1" disable; }
function ena(){ sudo "$1" enable; }
function mvcci(){ p=$(sed 's/\b\(.\)/\U\1/g;s/ //g'<<<"$1"); s=$(sed 's/ /_/g'<<<"$1"); sprkm "${p}Model"; sprkv "${s}_view"; sprkc "${p}Controller"; }
function random(){ echo $((RANDOM%${1-2})); }
